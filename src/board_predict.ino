        while ( !client.available() ) {}
        prediction = client.read();
        prediction |= CLOUD_PREDICTION;
    }
}

void predictOnBoard()
{
    unsigned int response[NUM_LABELS] = {};
    unsigned int minResponse = 65535;
    unsigned int secondMinResponse = 65535;

    for ( unsigned int i=0; i<DATA_SIZE; i++ )
        for ( byte l=0; l<NUM_LABELS; l++ )
            response[l] += abs(imageData[i] - model[l][i]);

    for ( byte l=0; l<NUM_LABELS; l++ )
    {
        if ( response[l] < minResponse )
        {
            prediction = l;
            secondMinResponse = minResponse;
            minResponse = response[l];
        }
        else if ( response[l] < secondMinResponse )
        {
            secondMinResponse = response[l];
        }
    }
    
    confidence = 1.0 - ((float)minResponse/(float)secondMinResponse);
}

/* BOARD ---------------------------------------------------------------------*/

void setup() 
{
    pinMode(LED_BUILTIN, OUTPUT);

    Serial.begin(115200);
    while ( !Serial ) {}

    WiFi.begin(WIFI_SSID, WIFI_PASSWORD);
    while ( WiFi.status() != WL_CONNECTED ) {}
    while ( !client.connect(server, PORT) ) {}
    digitalWrite(LED_BUILTIN, HIGH);
}

void loop() 
{
    if ( Serial.available() ) 
    {
        prediction = UNCERTAIN;
        Serial.readBytes(imageData, DATA_SIZE);
#ifndef CLOUD_ONLY
        predictOnBoard();
        if ( confidence < CONFIDENCE_THRESHOLD )
#endif
            predictOnCloud();
        Serial.write(prediction);
    }
    if ( !client.connected() ) 
    {
        client.stop();
        digitalWrite(LED_BUILTIN, LOW);
        while ( !client.connect(server, PORT) ) {}
        digitalWrite(LED_BUILTIN, HIGH);
    }
}
